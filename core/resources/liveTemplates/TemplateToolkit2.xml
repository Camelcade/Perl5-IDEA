        <variable name="CLOSETAG" expression="tt2CloseMarker()" defaultValue="" alwaysStopAt="false"/>
        <variable name="OPENTAG" expression="tt2OpenMarker()" defaultValue="" alwaysStopAt="false"/>
        <context>
            <option name="Template Toolkit 2.command" value="true"/>
        </context>
    </template>
    <template name="TRY" value="TRY $CLOSETAG$&#10;    $END$&#10;$OPENTAG$ END" description="TRY block"
              toReformat="false" toShortenFQNames="true">
        <variable name="CLOSETAG" expression="tt2CloseMarker()" defaultValue="" alwaysStopAt="false"/>
        <variable name="OPENTAG" expression="tt2OpenMarker()" defaultValue="" alwaysStopAt="false"/>
        <context>
            <option name="Template Toolkit 2.command" value="true"/>
        </context>
    </template>
    <template name="ELSIF" value="ELSIF $CONDITION$" description="ELSIF block" toReformat="false"
              toShortenFQNames="true">
        <variable name="CONDITION" expression="&quot;condition&quot;" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="Template Toolkit 2.command" value="false"/>
            <option name="Template Toolkit 2.command.elsif" value="true"/>
        </context>
    </template>
    <template name="ELSE" value="ELSE" description="ELSE" toReformat="false" toShortenFQNames="true">
        <context>
            <option name="Template Toolkit 2.command" value="false"/>
            <option name="Template Toolkit 2.command.elsif" value="true"/>
        </context>
    </template>
    <template name="CASE" value="CASE $CONDITION$" description="CASE branch" toReformat="false" toShortenFQNames="true">
        <variable name="CONDITION" expression="&quot;condition&quot;" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="Template Toolkit 2.command" value="false"/>
            <option name="Template Toolkit 2.command.case" value="true"/>
        </context>
    </template>
    <template name="CATCH" value="CATCH $TYPE$" description="CATCH branch" toReformat="false" toShortenFQNames="true">
        <variable name="TYPE" expression="&quot;type&quot;" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="Template Toolkit 2.command" value="false"/>
            <option name="Template Toolkit 2.command.elsif" value="false"/>
            <option name="Template Toolkit 2.command.catch" value="true"/>
        </context>
    </template>
    <template name="FINAL" value="FINAL" description="FINAL branch" toReformat="false" toShortenFQNames="true">
        <context>
            <option name="Template Toolkit 2.command" value="false"/>
            <option name="Template Toolkit 2.command.elsif" value="false"/>
            <option name="Template Toolkit 2.command.catch" value="true"/>
        </context>
    </template>
</templateSet>