<%flags>
    inherit=>'/top.mas'
</%flags>

<%method firstmethod>
<a href="http://www.<% $site %>.com"><% $label %></a>

    <%args>
        $site
        $label=>ucfirst($site)
    </%args>
</%method>

<%def firstsubcomponent>
<a href="http://www.<% $site %>.com"><% $label %></a>

    <%args>
        $site
        $label=>ucfirst($site)
    </%args>
</%def>
