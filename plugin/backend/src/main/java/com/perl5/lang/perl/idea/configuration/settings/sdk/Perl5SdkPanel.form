<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.perl5.lang.perl.idea.configuration.settings.sdk.Perl5SdkPanel">
  <grid id="27dc6" binding="myMainPanel" layout-manager="BorderLayout" hgap="5" vgap="0">
    <constraints>
      <xy x="20" y="20" width="252" height="40"/>
    </constraints>
    <properties>
      <maximumSize width="2147483647" height="34"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="4f99" class="javax.swing.JLabel" binding="myLabel">
        <constraints border-constraint="West"/>
        <properties>
          <text resource-bundle="messages/PerlBundle" key="perl.config.interpreter.label"/>
        </properties>
      </component>
      <component id="20dae" class="com.intellij.openapi.ui.ComboBox" binding="mySdkComboBox">
        <constraints border-constraint="Center"/>
        <properties>
          <maximumSize width="32767" height="30"/>
          <minimumSize width="98" height="30"/>
        </properties>
      </component>
      <grid id="a5181" binding="myToolBar" custom-create="true" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="East"/>
        <properties>
          <inheritsPopupMenu value="true"/>
          <maximumSize width="26" height="24"/>
          <minimumSize width="26" height="24"/>
          <preferredSize width="26" height="24"/>
          <requestFocusEnabled value="true"/>
        </properties>
        <border type="empty">
          <size top="3" left="0" bottom="0" right="0"/>
        </border>
        <children/>
      </grid>
    </children>
  </grid>
</form>
